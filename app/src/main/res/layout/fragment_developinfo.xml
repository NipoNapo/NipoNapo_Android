<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".ui.developinfo.DevelopinfoFragment">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical"
        tools:layout_editor_absoluteX="79dp"
        tools:layout_editor_absoluteY="148dp">

        <TextView
            android:id="@+id/di_text1"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="5dp"
            android:text="@string/di_text1"
            android:textAlignment="center"
            android:textSize="24sp" />

        <TextView
            android:id="@+id/di_text2"
            android:layout_width="match_parent"
            android:layout_height="74dp"
            android:text="@string/di_text2"
            android:textAlignment="center" />

        <TextView
            android:id="@+id/di_email"
            android:layout_width="326dp"
            android:layout_height="wrap_content"
            android:layout_marginLeft="8dp"
            android:text="@string/di_email"
            android:textAlignment="viewStart"
            android:textSize="12sp" />

        <TextView
            android:id="@+id/text_developinfo"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginStart="8dp"
            android:layout_marginTop="8dp"
            android:layout_marginEnd="8dp"
            android:textAlignment="center"
            android:textSize="20sp"
            android:visibility="invisible" />


    </LinearLayout>

</androidx.constraintlayout.widget.ConstraintLayout>